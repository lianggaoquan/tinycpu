E_00c02be0/0 .event negedge, v00bfed78_0;
E_00c02be0/1 .event posedge, v00c02c08_0;
E_00c02be0 .event/or E_00c02be0/0, E_00c02be0/1;
    .scope S_00c02a60;
T_0 ;
    %wait E_00c02be0;
    %load/vec4 v00bfed78_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v00bfecf8_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v00bfec88_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v00bfeb98_0;
    %assign/vec4 v00bfecf8_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v00bfec00_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %load/vec4 v00bfecf8_0;
    %addi 1, 0, 16;
    %assign/vec4 v00bfecf8_0, 0;
T_0.4 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_00c02990;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00bfee78_0, 0, 1;
T_1.0 ;
    %delay 50000, 0;
    %load/vec4 v00bfee78_0;
    %inv;
    %store/vec4 v00bfee78_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_00c02990;
T_2 ;
    %vpi_call 2 15 "$dumpfile", "counter.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_00c02990 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00bff0b8_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00bfefa0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00bfef48_0, 0, 1;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v00bfeee0_0, 0, 16;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v00bff0b8_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v00bfef48_0, 0, 1;
    %delay 300000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00bfef48_0, 0, 1;
